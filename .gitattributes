*.apk filter=lfs diff=lfs merge=lfs -text
*.so filter=lfs diff=lfs merge=lfs -text
*.dill filter=lfs diff=lfs merge=lfs -text
# Enforce LF line endings for code and config files
*.dart text eol=lf
*.yaml text eol=lf
*.xml text eol=lf
*.json text eol=lf
*.gradle text eol=lf
*.sh text eol=lf
*.bat text eol=crlf
